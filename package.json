	"license": "Apache-2.0",
	"homepage": "https://github.com/labor-digital/helferlein",
	"dependencies": {
		"clone": "^2.1.2"
	},
	"devDependencies": {
		"@types/jest": "24.9.0",
		"@types/node": "13.1.8",
		"jest": "24.9.0",
		"ts-jest": "24.3.0",
		"typedoc": "0.16.7",
		"typescript": "3.7.5"
	}
}
